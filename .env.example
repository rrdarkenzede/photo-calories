# OpenFoodFacts API (gratuite, pas besoin de clé)
NEXT_PUBLIC_OPENFOODFACTS_API=https://world.openfoodfacts.org

# Clarifai API pour la reconnaissance d'images
NEXT_PUBLIC_CLARIFAI_API_KEY=your_clarifai_key_here

# USDA FoodData Central API
NEXT_PUBLIC_USDA_API_KEY=your_usda_key_here

# Base de données (Supabase)
DATABASE_URL=your_database_url_here
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# JWT Secret
JWT_SECRET=your_jwt_secret_here

# Stripe (optionnel)
STRIPE_SECRET_KEY=your_stripe_secret_key_here
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here